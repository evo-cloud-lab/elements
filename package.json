{
    "name": "evo-elements",
    "description": "Evo Cloud Basic Elements",
    "version": "0.0.3",
    "author": "Yisui Hu <easeway@gmail.com>",
    "keywords": [
        "elements",
        "class", "object-oriented", "OOP",
        "log", "logging", "trace",
        "configuration",
        "schedule"
    ],
    "scripts": {
        "test": "mocha -R spec test/*-test.js",
        "cov": "mocha -r blanket -R html-cov test/*-unit-test.js >_coverage.html"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/evo-cloud/elements"
    },
    "main": "./index",
    "dependencies": {
        "js-class": "0.0.2",
        "underscore": "1.5.1",
        "js-yaml": "2.1.0",
        "winston": "0.7.2",
        "winston-syslog": "0.2.2",
        "validator": "1.3.0",
        "debug": "0.7.2",
        "nomnom": "1.6.1",
        "colors": "0.6.2"
    },
    "devDependencies": {
        "mocha": "1.12.0",
        "sandboxed-module": "0.2.1",
        "blanket": "1.1.6"
    },
    "engines": {
        "node": ">=0.10",
        "npm": "1"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "https://github.com/evo-cloud/elements/raw/master/LICENSE"
        }
    ],
    "config": {
        "blanket": {
            "onlyCwd": true,
            "pattern": "//^((?!node_modules).)*/lib/.+$/"
        }
    }
}